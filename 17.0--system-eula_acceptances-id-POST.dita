<reference id="api-17.0--system-eula_acceptances-id-POST" xml:lang="en-us">
<title>POST /system/eula_acceptances/{id}</title>
<shortdesc>Updates an individual EULA acceptance.</shortdesc>
<prolog>
<metadata>
<keywords>
<apiname>/system/eula_acceptances/{id}</apiname>
<keyword>POST</keyword>
<keyword>API</keyword>
